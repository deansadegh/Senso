<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.usersensorik.login.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.usersensorik.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_forgotpassFragment"
            app:destination="@id/forgotpassFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.usersensorik.intro.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_exploreFragment"
            app:destination="@id/exploreFragment" />
        <action
            android:id="@+id/action_homeFragment_to_questionFragment"
            app:destination="@id/questionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_question2Fragment"
            app:destination="@id/question2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.usersensorik.intro.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" >
        <action
            android:id="@+id/action_exploreFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_explore_homeFragment"
            app:destination="@id/explore_homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.usersensorik.intro.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_chengepasswordFragment"
            app:destination="@id/chengepasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.usersensorik.explore.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.usersensorik.explore.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />
    <fragment
        android:id="@+id/explore_homeFragment"
        android:name="com.example.usersensorik.explore.ExplorehomeFragment"
        android:label="fragment_explore_home"
        tools:layout="@layout/fragment_explore_home" />
    <fragment
        android:id="@+id/forgotpassFragment"
        android:name="com.example.usersensorik.login.ForgotpassFragment"
        android:label="fragment_forgotpass"
        tools:layout="@layout/fragment_forgotpass" />
    <fragment
        android:id="@+id/chengepasswordFragment"
        android:name="com.example.usersensorik.intro.ChengepasswordFragment"
        android:label="fragment_chengepassword"
        tools:layout="@layout/fragment_chengepassword" />
    <fragment
        android:id="@+id/questionFragment"
        android:name="com.example.usersensorik.intro.QuestionFragment"
        android:label="fragment_question"
        tools:layout="@layout/fragment_question" />
    <fragment
        android:id="@+id/question2Fragment"
        android:name="com.example.usersensorik.intro.Question2Fragment"
        android:label="fragment_question2"
        tools:layout="@layout/fragment_question2" />
    <fragment
        android:id="@+id/tabFragment"
        android:name="com.example.usersensorik.intro.TabFragment"
        android:label="fragment_tab"
        tools:layout="@layout/fragment_tab" />
</navigation>